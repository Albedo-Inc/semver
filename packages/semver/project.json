{
  "root": "packages/semver",
  "sourceRoot": "packages/semver/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "version": {
      "executor": "./dist/packages/semver:version",
      "options": {
        "noVerify": true,
        "postTargets": ["semver:publish", "semver:github"]
      }
    },
    "github": {
      "executor": "./dist/packages/semver:github",
      "options": {
        "tag": "${tag}",
        "notesFile": "packages/semver/CHANGELOG.md"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/semver/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/semver/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "outputPath": "dist/packages/semver",
        "tsConfig": "packages/semver/tsconfig.lib.json",
        "packageJson": "packages/semver/package.json",
        "main": "packages/semver/src/index.ts",
        "updateBuildableProjectDepsInPackageJson": false,
        "assets": [
          "README.md",
          "LICENSE",
          {
            "input": "packages/semver",
            "glob": "**/files/**",
            "output": "/"
          },
          {
            "input": "packages/semver",
            "glob": "**/files/**/.gitkeep",
            "output": "/"
          },
          {
            "input": "packages/semver",
            "glob": "**/*.json",
            "ignore": ["**/tsconfig*.json", "project.json", "node_modules/**"],
            "output": "/"
          },
          {
            "input": "packages/semver",
            "glob": "**/*.d.ts",
            "ignore": ["node_modules/**"],
            "output": "/"
          }
        ]
      },
      "outputs": ["{options.outputPath}"]
    }
  },
  "tags": []
}
